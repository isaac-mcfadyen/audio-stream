{
	"app": {
		"security": {
			"csp": null
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 600,
				"resizable": true,
				"title": "Audio Stream",
				"width": 800
			}
		],
		"trayIcon": {
			"iconPath": "icons/32x32.png",
			"tooltip": "Audio Stream"
		}
	},
	"build": {
		"beforeBuildCommand": "pnpm run build",
		"beforeDevCommand": "pnpm run dev",
		"devUrl": "http://localhost:5173",
		"frontendDist": "../build"
	},
	"bundle": {
		"active": true,
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"targets": "all"
	},
	"identifier": "com.tauri.dev",
	"productName": "audio-stream",
	"version": "0.1.0"
}
